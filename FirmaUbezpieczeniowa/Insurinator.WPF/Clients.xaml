<Window x:Class="Insurinator.WPF.Clients"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Insurinator.WPF"
        mc:Ignorable="d"
        Title="Clients" Height="300" Width="300">
    <Grid>
        <Button Content="Home" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <ListBox HorizontalAlignment="Left" Height="244" Margin="0,25,0,0" VerticalAlignment="Top" ItemsSource="{Binding  ClientsList}" Width="164"/>

    </Grid>
</Window>
